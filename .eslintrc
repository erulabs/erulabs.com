{
  "parser": "babel-eslint",
  "extends": "standard",
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "rules": {
    'no-unused-vars': 0
  },
  "plugins": [
    "react"
  ],
  "settings": {
    "react": {
      "pragma": "React"
    }
  },
  "globals": {
    "React": false,
    "ReactDOM": false,
    "THREE": false
  }
}
